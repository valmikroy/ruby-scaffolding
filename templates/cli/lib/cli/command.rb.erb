require '<%= snake_name %>'
require '<%= snake_name %>/helper'
require 'rspec'

include <%= camel_name %>::Helper

module <%= camel_name %>
  class CLI
    class Command
      attr_reader :options


      def initialize(command_arg, options = {})
        @options = options
        <%= camel_name %>.logger.info("Command argument is #{command_arg}")
      end

      def call
        <%= camel_name %>.logger.info('Verify executed')
        <%= camel_name %>.logger.info("Example option is #{@options[:example_option]}")
        <%= camel_name %>.logger.debug("Verbose options is #{@options[:verbose]}")
        <%= camel_name %>.logger.info("Dry options is #{@options[:dry]}") if <%= camel_name %>.dry
        true
      end


    end
  end
end
